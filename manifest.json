{
    "manifest_version": 2,
    "name": "nvOptimize",
    "description": "This extension allows the user to optimize the web page in nvOptimize editor.",
    "version": "1.0",
    "browser_action": {
        "default_icon": "favicon.png",
        "default_title": "nvOptimize"
        // "default_popup": "popup.html"
    },
    "background": {
        "persistent": false,
        "scripts": [
            "background.js"
        ]
    },
    "permissions": [
        "activeTab",
        "storage"
    ]
}